<h1> These are the Posts on this page </h1>
<br>
<table class="table">
  <thead>
    <tr>
      
      <th>Post</th>
      <th>Date</th>
      <th>Rationale</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post|  %>
    <tr>
      <th scope="row"><%= post.id %></th>
      <td><%= post.date %></td>
      <td><%= post.rationale %></td>      
    </tr>  
   <% end %> 
  </tbody>
</table>